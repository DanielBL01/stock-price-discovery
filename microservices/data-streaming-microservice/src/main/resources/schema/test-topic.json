{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "content": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "summary": {
          "type": "string"
        },
        "link": {
          "type": "string"
        }
      }
    },
    "label": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    }
  },
  "required": ["id", "content", "label"]
}